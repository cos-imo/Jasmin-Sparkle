from functions require "sparkle384_11.jazz"

export
fn sparkle11(reg u64 state) {
  stack u32[8] SL;
  stack u32[4] SR;
  reg ptr SL_ptr;
  reg ptr SR_ptr;
  inline int i;

  for i=0 to 8 {
    SL[i] = (u32)[state + 4*i];
  }

  for i=0 to 4 {
    SR[i] = (u32)[state + 32 + 4*i];
  }

  SL_ptr = state;
  SR_ptr = state + 32;

  SL, SR = sparkle384_11(SL_ptr, SR_ptr, 7);

  for i=0 to 8 {
    (u32)[state + 4*i] = SL[i];
  }

  for i=0 to 4 {
    (u32)[state + 32 + 4*i] = SR[i];
  }
}
