inline 
fn sparkle_pre_padding(reg u32[8] first_tab, reg u32[4] second_tab) -> reg u32[6], reg u32[6] {
  reg u32[6] x;
  reg u32[6] y;
  
  x[0] = first_tab[0];
  y[0] = first_tab[1];
  x[1] = first_tab[2];
  y[1] = first_tab[3];
  x[2] = first_tab[4];
  y[2] = first_tab[5];
  x[3] = first_tab[6];
  y[3] = first_tab[7];
  x[4] = second_tab[0];
  y[4] = second_tab[1];
  x[5] = second_tab[2];
  y[5] = second_tab[3];

  return x,y;
}
