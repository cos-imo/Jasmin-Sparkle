inline
fn branch_permutation (reg ptr u32[8] SL, reg ptr u32[4] SR) -> reg ptr u32[8], reg ptr u32[4] {

  reg u32 xy_0_save;

  xy_0_save = SL[0];

  SL[0] = SR[0];
  SR[0] = SL[2];
  SL[2] = SR[2];
  SR[2] = SL[4];
  SL[4] = SL[6];
  SL[6] = xy_0_save;

  xy_0_save = SL[1];

  SL[1] = SR[1];
  SR[1] = SL[3];
  SL[3] = SR[3];
  SR[3] = SL[5];
  SL[5] = SL[7];
  SL[7] = xy_0_save;

  return SL, SR;
}
